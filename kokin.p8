-- main

t=0
egg=nil
game_state=nil
--debug=true

pet_colors={
	{main=11,shadow=3},
	{main=8,shadow=2},
	{main=10,shadow=9},
	{main=12,shadow=13}
}

// do not put names longer
// than 8 characters
pet_names={
 //........
	"terra",
	"jupiter",
	"zodiac",
	"saturn",
	"nebula",
	"winter",
	"atlas",
	"ceres",
	"gamma",
	"nova",
	"pyros",
}

pet_sex={"male","female"}

pet_data={}
active_pet=nil
sel_action=1

actions={
	{txt="feed", icon=100},
	{txt="play", icon=101}
}

function _init()
	reset_game()
end

function _update()
	if game_state=="hatch" then
		update_hatch()
	elseif game_state=="room" then
		update_room()
	end
end

function _draw()
	cls(0)
	
	if game_state=="hatch" then
		draw_hatch(egg)
	elseif game_state=="room" then
		draw_room()
	end
end


function reset_game()
	hatch_new_egg()
	-- todo: tmp
	init_room()
	--game_state="room"
end

function gen_pet_data()
	local data={
		clrs=rnd(pet_colors),
		sex=rnd(pet_sex),
		name=rnd(pet_names)
	}
	
	return data
end

function print_c(text,x,y,clr)
	local width=#text*4
	print(text,x-width/2,y,clr)
end
